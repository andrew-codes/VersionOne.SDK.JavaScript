{
  "name": "v1sdk",
  "version": "0.3.6",
  "description": "VersionOne API Client for JavaScript",
  "keywords": [
    "VersionOne",
    "V1",
    "SDK"
  ],
  "homepage": "https://github.com/versionone/VersionOne.SDK.JavaScript",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:versionone/VersionOne.SDK.JavaScript.git"
  },
  "scripts": {
    "preparepublish": "npm run lint && npm test && npm run build",
    "build": "babel -d dist src",
    "lint": "eslint ./src",
    "test": "babel -d dist src --plugins rewire && babel -d tmp specs && mocha ./tmp/**/*.specs.js"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "elementtree": "*",
    "invariant": "^2.2.0",
    "request": "*"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-cli": "6.3.17",
    "babel-core": "6.3.21",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "babel-plugin-rewire": "^1.0.0-beta-3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "6.3.19",
    "chai": "3.4.1",
    "chai-as-promised": "5.2.0",
    "coffee-loader": "0.7.2",
    "coffee-script": "1.10.0",
    "eslint": "1.10.3",
    "mocha": "2.3.4",
    "sinon": "2.0.0-pre"
  }
}
