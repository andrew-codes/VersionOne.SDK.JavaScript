{
  "name": "v1sdk",
  "version": "1.0.1",
  "description": "VersionOne API Client for JavaScript",
  "license": "MIT",
  "keywords": [
    "VersionOne",
    "V1",
    "SDK"
  ],
  "homepage": "https://github.com/versionone/VersionOne.SDK.JavaScript",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:versionone/VersionOne.SDK.JavaScript.git"
  },
  "scripts": {
	"clean": "rm -rf dist",
	"clean:specs": "rm -rf dist/*.specs.js dist/**/*.specs.js",
	"prebuild": "npm run clean && npm run lint && npm run test",
	"build": "babel src -d dist --ignore *.specs.js",
	"lint": "eslint ./src",
	"pretest": "babel src -d dist --ignore *.specs.js --plugins rewire && babel src -d dist --only *.specs.js",
	"test": "mocha dist/**/*.specs.js",
	"posttest": "npm run clean:specs",
	"preparepublish": "npm run build"
  },
  "dependencies": {
    "btoa": "1.1.2",
    "invariant": "2.2.0"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-cli": "6.3.17",
    "babel-core": "6.3.21",
    "babel-eslint": "4.1.6",
    "babel-plugin-rewire": "1.0.0-beta-3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "babel-runtime": "6.3.19",
    "chai": "3.4.1",
    "chai-as-promised": "5.2.0",
    "eslint": "1.10.3",
    "mocha": "2.3.4",
    "sinon": "2.0.0-pre"
  }
}
